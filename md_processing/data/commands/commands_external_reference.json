{
  "Command Specifications": {
    "Create External Reference": {
      "display_name": "External Reference",
      "qn_prefix": "ExtRef",
      "family": "External Reference",
      "description": "Create or update External Reference Elements - or sub-types Related Media, Cited Documents, External Data Source and External Model Source.",
      "verb": "Create",
      "find_method": "ExternalReference.find_external_references",
      "Attributes": [
        {
          "Display Name": {
            "variable_name": "display_name",
            "attr_labels": "Name",
            "description": "Name of the external reference.",
            "style": "ID",
            "input_required": true,
            "min_cardinality": 1
          }
        },
        {
          "Description": {
            "variable_name": "description",
            "attr_labels": "",
            "description": "Description of the contents of a product."
          }
        },
        {
          "Reference Title": {
            "variable_name": "reference_title",
            "attr_labels": "Title",
            "description": "Title of the external reference."
          }
        },
        {
          "Reference Abstract": {
            "variable_name": "reference_abstract",
            "attr_labels": "Abstract",
            "description": "Abstract for the remote reference."
          }
        },
        {
          "Authors": {
            "variable_name": "authors",
            "attr_labels": "Author",
            "description": "A list of authors.",
            "style": "Simple List"
          }
        },
        {
          "Organization": {
            "variable_name": "organization",
            "attr_labels": "",
            "description": "Organization owning the external reference."
          }
        },
        {
          "Sources": {
            "variable_name": "reference_sources",
            "attr_labels": "Reference Sources",
            "description": "A map of source strings.",
            "style": "Dictionary"
          }
        },
        {
          "License": {
            "variable_name": "license",
            "attr_labels": "",
            "description": "The license associated with the external reference."
          }
        },
        {
          "Copyright": {
            "variable_name": "copyright",
            "attr_labels": "",
            "description": "The copy right associated with the external reference."
          }
        },
        {
          "Attribution": {
            "variable_name": "category",
            "attr_labels": "",
            "description": "Attribution string to describe the external reference."
          }
        }
      ]
    },
