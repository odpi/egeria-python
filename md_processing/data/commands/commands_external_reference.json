    "Link External Reference Link": {
      "display_name": "External Reference Link",
      "qn_prefix": "",
      "alternate_names": "Referenceable->External Reference; External Reference;  External Data Source; External Model Source",
      "family": "External Reference",
      "description": "Link an external reference to a referenceable.",
      "verb": "Link",
      "upsert": false,
      "attach": true,
      "find_method": "",
      "Attributes": [
        {
          "Element Name": {
            "variable_name": "element_name",
            "attr_labels": "Referenceable",
            "existing_element": "Referenceable",
            "description": "A referenceable to link.",
            "style": "Reference Name",
            "input_required": true,
            "min_cardinality": 1
          }
        },
        {
          "External Reference": {
            "variable_name": "external_reference",
            "attr_labels": "",
            "existing_element": "ExternalReference",
            "description": "The external reference to link to.",
            "style": "Reference Name",
            "input_required": true,
            "min_cardinality": 1
          }
        },
        {
          "Label": {
            "variable_name": "label",
            "attr_labels": "",
            "description": "Labels the link between the referenceable and the external reference."
          }
        },
        {
          "Description": {
            "variable_name": "description",
            "attr_labels": "",
            "description": "A description of the link."
          }
        }
      ]
    },
