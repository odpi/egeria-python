          }
        },
        {
          "Expression": {
            "variable_name": "expression",
            "attr_labels": "",
            "description": "An expression describing the relationship.",
            "min_cardinality": 1,
            "level": "Advance"
          }
        },
        {
          "Confidence": {
            "variable_name": "confidence",
            "attr_labels": "",
