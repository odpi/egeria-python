          }
        },
        {
          "Steward": {
            "variable_name": "steward",
            "attr_labels": "",
            "description": "Steward responsible.",
            "level": "Advance"
          }
        },
        {
          "Source": {
            "variable_name": "source",
            "attr_labels": "",
            "description": "Source of the relationship",
            "level": "Advance"
          }
